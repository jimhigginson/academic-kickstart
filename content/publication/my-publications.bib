  issn = {02664356},
  doi = {10.1016/j.bjoms.2019.10.009},
  abstract = {Introduction Cancellation of scheduled surgery is undesirable, in the same time, emergency admissions should be managed efficiently to deliver optimal patient care. The aim of this service evaluation was to analyse the impact of cancelling elective patients on the Elective maxillofacial theatres to accommodate pending trauma patients. Audit standards: 1. Trauma cases should be done ASAP within 24 hours. 2. Rebooking time for elective cases should be done within 28 days Methods The theatre lists over 6 months was analysed: 1. How many patients were cancelled and what procedure they required 2. Rebooking time for elective cases. 3. Costs of cancelling the patients verses the addition of trauma patients 4. Time delay for emergency cases Results The number of total elective patients cancelled over 6 months on theatre list was 34. From this, 38\% of patients had their surgery after 28 days. The number of trauma cases added was 19 in this period. It was found that patients admitted on Friday had the longest wait of 5 days. The average tariff balance paid to do the trauma cases over the elective cases was \textsterling 2860. Conclusions There was a significant delay in providing the trauma surgery when the patient was admitted over the weekend. There was a notable disruption to the elective list with the number of patients that had to be cancelled and rebooked. Suggestions for improvement: 1. Improve the CEPOD theatre access, especially over the weekend, dedicated theatre slots in CEPOD and dedicated trauma slot(s) on elective lists.},
  copyright = {All rights reserved},
  file = {/Users/jim/Zotero/storage/CXTG4N74/Ali et al. - 2019 - Operative procedure code (OPCS) consistency and it.pdf},
  journal = {British Journal of Oral and Maxillofacial Surgery},
  keywords = {Abstract},
  language = {en},
  number = {10}
}

@article{andersonTumorMicroenvironment2020,
  title = {The Tumor Microenvironment},
  author = {Anderson, Nicole M},
  year = {2020},
  month = aug,
  volume = {30},
  pages = {R921-925},
  doi = {10.1016/j.cub.2020.06.081},
  abstract = {A tumor is not simply a group of cancer cells, but rather a heterogeneous collection of infiltrating and resident host cells, secreted factors and extracellular matrix. Tumor cells stimulate significant molecular, cellular and physical changes within their host tissues to support tumor growth and progression. An emerging tumor microenvironment is a complex and continuously evolving entity. The composition of the tumor microenvironment varies between tumor types, but hallmark features include immune cells, stromal cells, blood vessels, and extracellular matrix. It is believed that the "tumor microenvironment is not just a silent bystander, but rather an active promoter of cancer progression" (Truffi et al., 2020). Early in tumor growth, a dynamic and reciprocal relationship develops between cancer cells and components of the tumor microenvironment that supports cancer cell survival, local invasion and metastatic dissemination. To overcome a hypoxic and acidic microenvironment, the tumor microenvironment coordinates a program that promotes angiogenesis to restore oxygen and nutrient supply and remove metabolic waste. Tumors become infiltrated with diverse adaptive and innate immune cells that can perform both pro- and anti- tumorigenic functions (Figure 1). An expanding literature on the tumor microenvironment has identified new targets within it for therapeutic intervention.},
  file = {/Users/jim/Zotero/storage/TYUP5RC3/Anderson - The tumor microenvironment.pdf},
  journal = {Curr Biol},
  keywords = {Review,Tumour Micro-Environment},
  language = {en},
  number = {16}
}

