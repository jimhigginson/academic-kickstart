@article{watesUrgentOralMaxillofacial2019,
  title = {Urgent Oral and Maxillofacial Surgery in the Pregnant Patient: A Review of the Literature},
  shorttitle = {Urgent Oral and Maxillofacial Surgery in the Pregnant Patient},
  author = {Wates, E. and Ria, B. and Samara, E. and Higginson, J.},
  year = {2019},
  month = may,
  volume = {48},
  pages = {70},
  issn = {09015027},
  doi = {10.1016/j.ijom.2019.03.214},
  abstract = {Background: Pregnant patients are presenting more frequently to OMFS for urgent treatment as a consequence of denied or delay in care in the primary setting; as a result, they often present with a more severe infection requiring drainage under general anesthesia or present after analgesic overdose. Similarly, the incidence of head and neck cancers in the younger female population is increasing and women are often conceiving at later ages. This presents an ethical dilemma of managing pregnant patients with head and neck cancer, balancing optimal treatment for the mother against protecting the fetus from harm. Objectives: The aims of this literature review were to explore the evidence base behind the safety of carrying out urgent dental treatment in pregnant patients. We also explored the evidence for treatment options available to patients diagnosed with oral cancer in pregnancy. Methods: The authors carried out a literature search in October 2018 using Medline. Findings: Barriers to primary care dentists providing urgent dental care to pregnant patients include: inability to manage this anxious patient group; lack of historical evidence on the safety to the fetus; lack of continuing professional development in this area; potential litigation risks; a fear of inducing labor. Conclusion: The treatment options available for the patient diagnosed with oral cancer in pregnancy include: termination of pregnancy; early delivery via cesarean section due to advances in neonatology; postpone treatment until after delivery; or to provide suboptimal treatment during pregnancy. We review the evidence we have available for these treatment options to help support patient autonomy.},
  copyright = {All rights reserved},
  file = {/Users/jim/Zotero/storage/J9FF8X6T/Wates et al. - 2019 - Urgent oral and maxillofacial surgery in the pregn.pdf},
  journal = {International Journal of Oral and Maxillofacial Surgery},
  keywords = {Abstract},
  language = {en}
}

